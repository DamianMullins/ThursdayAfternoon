@inherits NancyRazorViewBase<ThursdayAfternoon.ViewModels.Presentation.ViewPresentationViewModel>
@{
    Layout = "shared/_layout_full_width";
    ViewBag.Title = "Edit" + @Model.Title;
    bool isFirst = true;
}

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-title">@Model.Title</h1>
        </div>
    </div>
</div>

<div id="carousel-example-generic" class="carousel slide">
    <div class="carousel-inner">
        @foreach (var slide in Model.Slides)
        {
            <div class="item @(isFirst ? "active" : "")">
                @*<img src="data:image/png;base64," alt="...">*@
                <div class="carousel-caption">
                    <h3>@slide.Title</h3>
                    <p>@Html.Raw(slide.Content)</p>
                </div>
            </div>
            isFirst = false;
        }
    </div>

    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev"><span class="icon-prev"></span></a>
    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next"><span class="icon-next"></span></a>
</div>

<p><a href="#" class="make-fullscreen hidden-sm" data-target="carousel">Fullscreen</a></p>
